{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}
